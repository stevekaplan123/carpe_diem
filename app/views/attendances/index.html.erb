<h1>Listing Attendances</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @attendances.each do |attendance| %>
      <tr>
        <td><%= link_to attendance.user.name, attendance.user %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
